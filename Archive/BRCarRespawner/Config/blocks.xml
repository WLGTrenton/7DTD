<configs>
	<append xpath="/blocks">	
<!-- CarRespawner-->
		<block name="carRespawner">
			<property name="CustomIcon" value="decoCarTireStack" />
			<property name="CustomIconTint" value="09ff00"/>
			<property name="Extends" value="decoEntityMetalMaster"/>			
			<property name="IsDeveloper" value="true" />
			<property name="Class" value="PlantGrowing" />
			<property name="Material" value="Mair"/>			
			<property name="StabilitySupport" value="false"/>
		        <property name="Shape" value="Invisible"/>
            		<property name="Texture" value="250"/>	
			<property name="CanMobsSpawnOn" value="false" />
			<drop event="Destroy" count="0" />	
			<property name="IsTerrainDecoration" value="true" />
			<property name="PlantGrowing.Next" value="carsRandomHelper" />
			<property name="PlantGrowing.GrowthRate" value="240" /><!-- Time to respawn, 60 is about 1 day based on default time -->
			<property name="PlantGrowing.FertileLevel" value="0" />
			<property name="PlantGrowing.IsRandom" value="false" />
		</block>
